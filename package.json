    "@vercel/node": "^5.1.14",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "cypress": "^14.2.0",
    "eslint": "^9.23.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
