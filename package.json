    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "color-convert": "^3.0.1",
    "html-to-image": "^1.11.13",
    "lucide-vue-next": "^0.483.0",
    "pinia": "^3.0.1",
    "query-string": "^9.1.1",
    "reka-ui": "^2.1.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "universal-cookie": "^7.2.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "zod": "^3.24.2"
